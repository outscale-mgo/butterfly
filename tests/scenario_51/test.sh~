#!/bin/bash

#check that dperf work

BUTTERFLY_BUILD_ROOT=$1
BUTTERFLY_SRC_ROOT=$(cd "$(dirname $0)/../.." && pwd)
source $BUTTERFLY_SRC_ROOT/tests/functions.sh

network_connect 0 1
server_start 0
nic_add 0 3 42 sg-1
nic_add 0 4 42 sg-1
qemus_start 3 4
scp_to 1 $BUTTERFLY_SRC_ROOT/3rdparty/dpdk/usertools/
read
qemus_stop 3 4
server_stop 0
network_disconnect 0 1
return_result
